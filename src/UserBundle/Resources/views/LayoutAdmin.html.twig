    <link rel="stylesheet" href="{{ asset('fos/libs/OwlCarousel2-2.2.0/dist/assets/owl.carousel.min.css') }}">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/rateYo/2.3.2/jquery.rateyo.min.css">
    {% block stylesheets %}{% endblock %}
</head>
<body>


{% block body %}
    <header>

        <div class="cont">
            <div class="logo">
                <a href="#" id="logo-link">

						<img src="{{ asset('Profile_template/images/LOGO-JOBID.png') }}" alt="logo">

                </a>
            </div>

            <span class="menu-icon"><i class="fa fa-bars" aria-hidden="true"></i></span>
            <div class="nav">

                <span class="close-icon">X</span>

                <ul>
                    <!-- <li><a href="#">Imprimeurs </a></li>-->


                    {% if app.user %}
                        <li><a href="{{ path('admin_list_users') }}">Liste Utilisateurs</a></li>
                        <li><a href="{{ path('inscription_page') }}">Liste Annonces</a></li>
                        <li><a href="{{ path('admin_statistiques') }}">Statistiques   </a></li>
                        <li><a href="{{ path('fos_user_security_logout') }}">Deconnexion   </a></li>



                    {% endif %}

                </ul>
            </div>
        </div>

    </header>
{% block admin_main_welcome %}
    <div class="login-content">

        <div class="login-wrapp">

            <div class="img-login-center">

                <img src="{{ asset('Profile_template/images/LOGO-JOBID.png') }}" alt="logo" />

            </div>


            <h3 align="center">Bienvenue Administrateur : {{  app.user.username }}</h3>



        </div>
    </div>

{% endblock %}





{% endblock %}

<script src="https://code.jquery.com/jquery-1.11.3.min.js"></script>
