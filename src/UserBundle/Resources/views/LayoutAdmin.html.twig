    <link rel="stylesheet" href="{{ asset('fos/styles/chat.css') }}">
    <link rel="stylesheet" href="{{ asset('fos/styles/hover.css') }}">
    <link rel="stylesheet" href="{{ asset('fos/styles/bootstrap.min.css') }}">
